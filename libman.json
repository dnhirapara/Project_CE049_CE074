{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@4.5.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-toggle@2.2.2",
      "destination": "wwwroot/lib/bootstrap-toggle/"
    },
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "tinymce@5.5.1",
      "destination": "wwwroot/lib/tinymce/"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "twitter-bootstrap@4.5.3",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    }
  ]
}
